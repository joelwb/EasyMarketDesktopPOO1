<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>

<HBox minWidth="980.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="itensTable" maxHeight="1.7976931348623157E308" HBox.hgrow="ALWAYS">
        <columns>
          <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Produto" />
          <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Valor Total (R$)" />
            <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Quantidade" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</HBox>
