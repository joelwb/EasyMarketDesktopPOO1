<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="tableRelatorio" maxHeight="1.7976931348623157E308" HBox.hgrow="ALWAYS">
        <columns>
          <TableColumn editable="false" prefWidth="75.0" text="Nome" />
          <TableColumn editable="false" prefWidth="75.0" text="CPF" />
            <TableColumn fx:id="consumoCol" editable="false" prefWidth="75.0" text="Consumo" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <VBox alignment="TOP_CENTER" maxWidth="250.0" spacing="16.0">
         <children>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#getDetalhes" text="Detalhes">
               <VBox.margin>
                  <Insets top="24.0" />
               </VBox.margin>
            </Button>
            <Button maxWidth="1.7976931348623157E308" minWidth="175.0" mnemonicParsing="false" onAction="#getAllOrders" text="Todas as compras" />
         </children>
         <padding>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
         </padding>
      </VBox>
   </children>
</HBox>
